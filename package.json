{
  "name": "rimelight-components",
  "version": "1.1.3",
  "description": "My new Nuxt module",
  "repository": "Rimelight  Entertainment/rimelight-components",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "bun run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bun run lint && bun run test && bun run prepack && changelogen --release && bun publish && git push --follow-tags",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "workspaces": [
    "playground"
  ],
  "dependencies": {
    "@nuxt/image": "^1.11.0",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/ui": "^4.0.1",
    "date-fns": "^4.1.0",
    "nuxt": "^4.1.3",
    "tailwind-variants": "^3.1.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "motion-v": "^1.7.2",
    "oxlint": "^1.21.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide"
  ]
}
