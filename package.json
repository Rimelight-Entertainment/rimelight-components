{
  "name": "rimelight-components",
  "description": "A component library by Rimelight Entertainment.",
  "version": "2.0.41",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rimelight-Entertainment/rimelight-components.git"
  },
  "homepage": "https://rimelight.com/tools/rimelight-components",
  "license": "MIT",
  "keywords": [
    "rimelight",
    "typescript",
    "vue",
    "nuxt"
  ],
  "imports": {
    "#build/rimelight-components/*": "./.nuxt/rimelight-components/*.ts",
    "#build/rimelight-components.css": "./.nuxt/rimelight-components.css"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "style": "./dist/runtime/index.css",
      "import": "./dist/module.mjs"
    },
    "./runtime/*": "./dist/runtime/*",
    "./components/*": "./dist/runtime/components/*",
    "./composables/*": "./dist/runtime/composables/*",
    "./utils/*": {
      "types": "./dist/runtime/utils/*.d.mts",
      "import": "./dist/runtime/utils/*.js"
    }
  },
  "style": "./dist/runtime/index.css",
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ],
      "vite": [
        "./dist/vite.d.mts"
      ],
      "./runtime/*": [
        "./dist/runtime/index.d.ts"
      ],
      "components/*": [
        "./dist/runtime/components/*"
      ],
      "composables/*": [
        "./dist/runtime/composables/*"
      ],
      "utils": [
        "./dist/runtime/utils/index.d.ts"
      ],
      "utils/*": [
        "./dist/runtime/utils/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "bun run dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepack": "bun run build",
    "typecheck": "vue-tsc --noEmit && nuxt typecheck",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "format": "prettier . --check .",
    "format:fix": "prettier . --write .",
    "test": "vitest",
    "release": "release-it --ci"
  },
  "dependencies": {
    "@nuxt/image": "^1.11.0",
    "@nuxt/kit": "^4.2.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxtjs/i18n": "^10.1.2",
    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "^14.0.0",
    "date-fns": "^4.1.0",
    "defu": "^6.1.4",
    "nuxt": "^4.2.0",
    "pinia": "^3.0.4",
    "tailwind-variants": "^3.1.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.0.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.0",
    "@nuxt/test-utils": "^3.20.1",
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "motion-v": "^1.7.4",
    "oxlint": "^1.25.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "release-it": "^19.0.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.6",
    "vue-tsc": "^3.1.2"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "esbuild",
    "sharp",
    "vue-demi"
  ],
  "resolutions": {
    "rimelight-components": "workspace:*"
  }
}
